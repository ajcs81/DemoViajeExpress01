<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="icon" href="~/favicon.ico" type="image/x-icon">
    <title>@ViewBag.Title - Admin</title>

    <!-- Google font-->
    <link href="https://fonts.googleapis.com/css?family=Open+Sans:300,400,600,700,800" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css?family=Quicksand:500,700" rel="stylesheet">
    <link href="~/assets/resources/bower_components/bootstrap/css/bootstrap.min.css" rel="stylesheet" />
    <link href="~/assets/resources/assets/pages/waves/css/waves.min.css" rel="stylesheet" />
    <link href="~/assets/resources/assets/icon/feather/css/feather.css" rel="stylesheet" />
    <link href="~/assets/resources/bower_components/sweetalert/css/sweetalert.css" rel="stylesheet" />
    @*<link href="~/Content/assets/icon/font-awesome/css/font-awesome.min.css" rel="stylesheet" />*@
    <link href="~/assets/resources/assets/css/font-awesome-n.min.css" rel="stylesheet" />
    <link href="~/assets/resources/assets/css/component.css" rel="stylesheet" />
    <link href="~/assets/resources/bower_components/chartist/css/chartist.css" rel="stylesheet" />
    <link href="~/assets/resources/bower_components/datatables.net-bs4/css/dataTables.bootstrap4.min.css" />
    <link href="~/assets/resources/assets/pages/data-table/css/buttons.dataTables.min.css" />
    <!-- Data Table Css -->
    <link rel="stylesheet" href="~/assets/resources/bower_components/datatables.net-bs4/css/dataTables.bootstrap4.min.css">
    <link rel="stylesheet" href="~/assets/resources/assets/pages/data-table/css/buttons.dataTables.min.css">
    <link rel="stylesheet" href="~/assets/resources/bower_components/datatables.net-responsive-bs4/css/responsive.bootstrap4.min.css">
    <!--forms-wizard css-->
    <link rel="stylesheet" href="~/assets/resources/bower_components/jquery.steps/css/jquery.steps.css">
    <script href="~/assets/assets/pages/forms-wizard-validation/form-wizard.js"></script>
    <link href="~/assets/resources/assets/css/widget.css" rel="stylesheet" />
    <link href="~/assets/resources/assets/css/style.css" rel="stylesheet" />
    <link href="~/assets/resources/assets/css/pages.css" rel="stylesheet" />


    <!-- Leaflet assets -->
    <link rel="stylesheet" href="https://unpkg.com/leaflet@1.3.1/dist/leaflet.css"
          integrity="sha512-Rksm5RenBEKSKFjgI3a41vrjkw4EVPlJ3+OiI65vTjIdo9brlAacEuKOiQ5OFh7cOI1bkDwLqdLw3Zg0cRJAAQ=="
          crossorigin="" />

    <style>
        #map1 {
            min-height: 500px;
            max-height: 500px;
        }
    </style>
    @RenderSection("styles", required: false);
</head>
<body>
    <!-- [ Pre-loader ] start -->
    <div class="loader-bg">
        <div class="loader-bar"></div>
    </div>
    <!-- [ Pre-loader ] end -->
    <div id="pcoded" class="pcoded">
        <div class="pcoded-overlay-box"></div>
        <div class="pcoded-container navbar-wrapper">
            <!-- [ Header ] start -->
            <nav class="navbar header-navbar pcoded-header">
                <div class="navbar-wrapper">
                    <div class="navbar-logo">
                        <a href="@Url.Action("Index","Administrador")">
                            <img class="img-fluid" src="~/assets/resources/assets/images/logo.png" alt="Theme-Logo" />
                        </a>
                        <a class="mobile-menu" id="mobile-collapse" href="#!">
                            <i class="feather icon-menu icon-toggle-right"></i>
                        </a>
                        <a class="mobile-options waves-effect waves-light">
                            <i class="feather icon-more-horizontal"></i>
                        </a>
                    </div>
                    <div class="navbar-container container-fluid">
                        <ul class="nav-left">
                            <li class="header-search">
                                <div class="main-search morphsearch-search">
                                    <div class="input-group">
                                        <span class="input-group-prepend search-close">
                                            <i class="feather icon-x input-group-text"></i>
                                        </span>
                                        <input type="text" class="form-control" placeholder="Enter Keyword">
                                        <span class="input-group-append search-btn">
                                            <i class="feather icon-search input-group-text"></i>
                                        </span>
                                    </div>
                                </div>
                            </li>
                            <li>
                                <a href="#!" onclick="javascript:toggleFullScreen()" class="waves-effect waves-light">
                                    <i class="full-screen feather icon-maximize"></i>
                                </a>
                            </li>
                        </ul>
                        <ul class="nav-right">
                            <li class="header-notification">
                                <div class="dropdown-primary dropdown">
                                    <div class="dropdown-toggle" data-toggle="dropdown">
                                        <i class="feather icon-bell"></i>
                                        <span class="badge bg-c-red">5</span>
                                    </div>
                                    <ul class="show-notification notification-view dropdown-menu"
                                        data-dropdown-in="fadeIn" data-dropdown-out="fadeOut">
                                        <li>
                                            <h6>Notifications</h6>
                                            <label class="label label-danger">New</label>
                                        </li>
                                        <li>
                                            <div class="media">
                                                <img class="img-radius" src="~/assets/resources/assets/images/avatar-4.jpg"
                                                     alt="Generic placeholder image">
                                                <div class="media-body">
                                                    <h5 class="notification-user">John Doe</h5>
                                                    <p class="notification-msg">
                                                        Lorem ipsum dolor sit amet, consectetuer
                                                        elit.
                                                    </p>
                                                    <span class="notification-time">30 minutes ago</span>
                                                </div>
                                            </div>
                                        </li>
                                        <li>
                                            <div class="media">
                                                <img class="img-radius" src="~/assets/resources/assets/images/avatar-3.jpg"
                                                     alt="Generic placeholder image">
                                                <div class="media-body">
                                                    <h5 class="notification-user">Joseph William</h5>
                                                    <p class="notification-msg">
                                                        Lorem ipsum dolor sit amet, consectetuer
                                                        elit.
                                                    </p>
                                                    <span class="notification-time">30 minutes ago</span>
                                                </div>
                                            </div>
                                        </li>
                                        <li>
                                            <div class="media">
                                                <img class="img-radius" src="~/assets/resources/assets/images/avatar-4.jpg"
                                                     alt="Generic placeholder image">
                                                <div class="media-body">
                                                    <h5 class="notification-user">Sara Soudein</h5>
                                                    <p class="notification-msg">
                                                        Lorem ipsum dolor sit amet, consectetuer
                                                        elit.
                                                    </p>
                                                    <span class="notification-time">30 minutes ago</span>
                                                </div>
                                            </div>
                                        </li>
                                    </ul>
                                </div>
                            </li>

                            <li class="user-profile header-notification">

                                <div class="dropdown-primary dropdown">
                                    <div class="dropdown-toggle" data-toggle="dropdown">
                                        <img src="~/assets/resources/assets/images/avatar-4.jpg" class="img-radius"
                                             alt="User-Profile-Image">
                                        <span>John Doe</span>
                                        <i class="feather icon-chevron-down"></i>
                                    </div>
                                    <ul class="show-notification profile-notification dropdown-menu"
                                        data-dropdown-in="fadeIn" data-dropdown-out="fadeOut">
                                        <li>
                                            <a href="#!">
                                                <i class="feather icon-settings"></i> Settings

                                            </a>
                                        </li>
                                        <li>
                                            <a href="#">
                                                <i class="feather icon-user"></i> Profile

                                            </a>
                                        </li>
                                        <li>
                                            <a href="email-inbox.html">
                                                <i class="feather icon-mail"></i> My Messages

                                            </a>
                                        </li>
                                        <li>
                                            <a href="auth-lock-screen.html">
                                                <i class="feather icon-lock"></i> Lock Screen

                                            </a>
                                        </li>
                                        <li>
                                            <a href="auth-sign-in-social.html">
                                                <i class="feather icon-log-out"></i> Logout

                                            </a>
                                        </li>
                                    </ul>
                                </div>
                            </li>
                        </ul>
                    </div>
                </div>
            </nav>
            <div class="pcoded-main-container">
                <div class="pcoded-wrapper">
                    <!-- [ navigation menu ] start -->
                    <nav class="pcoded-navbar">
                        <div class="nav-list">
                            <div class="pcoded-inner-navbar main-menu">
                                <div class="pcoded-navigation-label">Navigation</div>
                                <ul class="pcoded-item pcoded-left-item">
                                    <li class="pcoded-hasmenu active pcoded-trigger">
                                        <a href="javascript:void(0)" class="waves-effect waves-dark">
                                            <span class="pcoded-micon"><i class="feather icon-home"></i></span>
                                            <span class="pcoded-mtext">Dashboard</span>
                                        </a>
                                        <ul class="pcoded-submenu">
                                            <li class="active">
                                                <a href="index.html" class="waves-effect waves-dark">
                                                    <span class="pcoded-mtext">Default</span>
                                                </a>
                                            </li>
                                            <li class="">
                                                <a href="default/dashboard-crm.html" class="waves-effect waves-dark">
                                                    <span class="pcoded-mtext">CRM</span>
                                                </a>
                                            </li>
                                            <li class="">
                                                <a href="default/dashboard-analytics.html"
                                                   class="waves-effect waves-dark">
                                                    <span class="pcoded-mtext">Analytics</span>
                                                    <span class="pcoded-badge label label-info ">NEW</span>
                                                </a>
                                            </li>
                                        </ul>
                                    </li>

                                </ul>
                                <div class="pcoded-navigation-label">Modulos</div>
                                <ul class="pcoded-item pcoded-left-item" id="modulos">
                                </ul>


                            </div>
                        </div>
                    </nav>
                    <!-- [ navigation menu ] end -->
                    <div class="pcoded-content">
                        <!-- [ breadcrumb ] start -->
                        <div class="page-header card">
                            <div class="row align-items-end">
                                <div class="col-lg-8">
                                    <div class="page-header-title">
                                        <i class="feather icon-home bg-c-blue"></i>
                                        <div class="d-inline">
                                            <h5>Dashboard</h5>
                                            <span>lorem ipsum dolor sit amet, consectetur adipisicing elit</span>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-lg-4">
                                    <div class="page-header-breadcrumb">
                                        <ul class=" breadcrumb breadcrumb-title">
                                            <li class="breadcrumb-item">
                                                <a href="index.html"><i class="feather icon-home"></i></a>
                                            </li>
                                            <li class="breadcrumb-item"><a href="#!">Dashboard</a> </li>
                                        </ul>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <!-- [ breadcrumb ] end -->

                        <div class="pcoded-inner-content">
                            <div class="main-body">
                                <div class="page-wrapper">
                                    <div class="page-body">
                                        <!-- [ page content ] start -->
                                        <div class="row">

                                            @RenderBody()


                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>


    <script src="~/assets/resources/bower_components/jquery/js/jquery.min.js"></script>
    <script src="~/assets/resources/bower_components/jquery-ui/js/jquery-ui.min.js"></script>
    <script src="~/assets/resources/bower_components/popper.js/js/popper.min.js"></script>
    <script src="~/assets/resources/bower_components/bootstrap/js/bootstrap.min.js"></script>
    <script src="~/assets/resources/assets/pages/waves/js/waves.min.js"></script>
    <script src="~/assets/resources/bower_components/jquery-slimscroll/js/jquery.slimscroll.js"></script>
    <script src="~/assets/resources/bower_components/sweetalert/js/sweetalert.min.js"></script>
    <script src="~/assets/resources/assets/js/modal.js"></script>
    <script src="~/assets/resources/assets/js/classie.js"></script>
    <script src="~/assets/resources/assets/js/modalEffects.js"></script>
    <script src="~/assets/resources/assets/pages/widget/wave/jquery.wavify.js"></script>
    <script src="~/assets/resources/assets/pages/widget/wave/TweenMax.min.js"></script>
    <script src="~/assets/resources/bower_components/chartist/js/chartist.js"></script>
    <!-- data-table js -->
    <script src="~/assets/resources/bower_components/datatables.net/js/jquery.dataTables.min.js"></script>
    <script src="~/assets/resources/bower_components/datatables.net-buttons/js/dataTables.buttons.min.js"></script>
    <script src="~/assets/resources/assets/pages/data-table/js/jszip.min.js"></script>
    <script src="~/assets/resources/assets/pages/data-table/js/pdfmake.min.js"></script>
    <script src="~/assets/resources/assets/pages/data-table/js/vfs_fonts.js"></script>
    <script src="~/assets/resources/assets/pages/data-table/extensions/colreorder/js/dataTables.colReorder.min.js"></script>
    <script src="~/assets/resources/bower_components/datatables.net-buttons/js/buttons.print.min.js"></script>
    <script src="~/assets/resources/bower_components/datatables.net-buttons/js/buttons.html5.min.js"></script>
    <script src="~/assets/resources/bower_components/datatables.net-bs4/js/dataTables.bootstrap4.min.js"></script>
    <script src="~/assets/resources/bower_components/datatables.net-responsive/js/dataTables.responsive.min.js"></script>
    <script src="~/assets/resources/bower_components/datatables.net-responsive-bs4/js/responsive.bootstrap4.min.js"></script>
    <!--Forms - Wizard js-->
    <script src="~/assets/resources/bower_components/jquery.cookie/js/jquery.cookie.js"></script>
    <script src="~/assets/resources/bower_components/jquery.steps/js/jquery.steps.js"></script>
    <script src="~/assets/resources/bower_components/jquery-validation/js/jquery.validate.js"></script>

    <script src="~/assets/resources/assets/pages/forms-wizard-validation/form-wizard.js"></script>
    <script src="~/assets/resources/assets/pages/data-table/extensions/colreorder/js/colreorder-custom.js"></script>
    <script src="~/assets/resources/assets/js/pcoded.min.js"></script>
    <script src="~/assets/resources/assets/js/vertical/vertical-layout.min.js"></script>
    <script src="~/assets/resources/assets/js/jquery.mCustomScrollbar.concat.min.js"></script>
    @*<script src="~/Content/assets/pages/dashboard/custom-dashboard.min.js"></script>*@
    @*<script src="~/Content/assets/pages/widget/widget-data.js"></script>*@
    <script src="~/assets/resources/assets/js/script.js"></script>
    @*<script src="~/assets/js/jquery.validate.js"></script>*@
    <!-- Leaflet assets js-->
    <script src="https://unpkg.com/leaflet@1.3.1/dist/leaflet.js"
            integrity="sha512-/Nsx9X4HebavoBvEBuyp3I7od5tA0UzAxs+j83KgC8PU0kgB4XiK4Lfe4y4cgBtaRJQEIFCW+oC506aPT2L1zw=="
            crossorigin=""></script>
    @RenderSection("scripts", required: false);
